pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    }
     {
        stage('checkout') {
            steps {
                echo 'Hello checkout'
            }
        }
    }
       {
        stage('build') {
            steps {
                echo 'Hello build'
            }
        }
    }


}
